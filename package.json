{
  "name": "react-pure.css",
  "version": "0.1.2",
  "description": "React Pure.CSS",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate": "npm-run-all --parallel validate-webpack:*",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
