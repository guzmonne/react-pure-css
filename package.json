  "author": "Guzman Monne",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel": "^6.5.2",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.11.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "hjs-webpack": "^8.3.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^2.3.0",
    "postcss-loader": "^0.9.1",
