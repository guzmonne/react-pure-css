{
  "name": "react-pure-css",
  "version": "1.0.0",
  "description": "React Pure.CSS components",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate": "npm-run-all --parallel validate-webpack:*",
    "validate-webpack:dev": "./node_modules/.bin/webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "./node_modules/.bin/webpack-validator webpack.config.js --env.prod",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/favicon.ico dist",
    "clean-and-copy": "npm-run-all --serial clean-dist copy-files",
    "prestart": "npm run clean-and-copy",
    "start": "./node_modules/.bin/webpack-dev-server --env.dev --content-base dist",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build": "./node_modules/.bin/webpack --env.dev",
    "build:prod": "./node_modules/.bin/webpack --env.prod"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/guzmonne/react-pure-css.git"
  },
  "keywords": [
    "React",
    "Pure.CSS",
    "CSS",
    "Components"
  ],
  "author": "Guzman Monne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guzmonne/react-pure-css/issues"
  },
  "homepage": "https://github.com/guzmonne/react-pure-css#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015-webpack": "^6.4.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "npm-run-all": "^2.3.0",
    "opt-cli": "^1.5.1",
    "rimraf": "^2.5.4",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.3"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}
